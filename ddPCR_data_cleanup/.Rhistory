proj.vec<-function(x,y){
angle<-(x%*%y)/(euclid(x)*euclid(y))
angle<-pmin(pmax(angle,-1.0),1.0)
angle<-acos(angle)
emag<-euclid(x)*cos(angle)
return(c(angle,emag))
}
